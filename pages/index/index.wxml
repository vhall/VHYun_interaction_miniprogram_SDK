<!-- pages/index.wxml -->
<view class='item'>
	<span>inavId</span>
	<span class="input-span">
		<input
		 id='inavId'
		 bindinput='getinput'
		 type='text'
		 value='{{inavId}}'
		/>
	</span>
</view>
<view class='item'>
	<span>appId</span>
	<span class="input-span">
		<input
		 id='appId'
		 bindinput='getinput'
		 type='text'
		 value='{{appId}}'
		/>
	</span>
</view>
